                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dlh.hqy.psclient.LoginActivity"
            android:label="@string/title_activity_login" >
